@model YG.WorkArea.ViewModels.WorkVMs.WorkVM

<wt:form vm="@Model">
	<wt:quote>@Model.Localizer["DeleteConfirm"]</wt:quote>
<wt:row items-per-row="ItemsPerRowEnum.Two">
<wt:display field="Entity.Name" />
<wt:display field="Entity.WithDraw" />
<wt:display field="Entity.Starter.CodeAndName" />
<wt:display field="Entity.Receiver.CodeAndName" />
<wt:display field="Entity.WorkStatus" />
<wt:display field="Entity.Template.Name" />
</wt:row>
    <wt:hidden field="Entity.ID" />
    <wt:row align="AlignEnum.Right">
        <wt:submitbutton theme=" ButtonThemeEnum.Warm" text="删除"/>
        <wt:closebutton />
    </wt:row>
</wt:form>
