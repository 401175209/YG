@model YG.AdminArea.ViewModels.AdminVMs.AdminVM

<wt:form vm="@Model">
	<wt:quote>@Model.Localizer["DeleteConfirm"]</wt:quote>
<wt:row items-per-row="ItemsPerRowEnum.Two">
<wt:display field="Entity.Organ.OrganName" />
<wt:display field="Entity.MAC" />
<wt:display field="Entity.ITCode" />
<wt:display field="Entity.Name" />
<wt:display field="Entity.Sex" />
<wt:display field="Entity.CellPhone" />
<wt:display field="Entity.PhotoId" label-text="签名" />
<wt:display field="Entity.IsValid" />


</wt:row>
    <wt:hidden field="Entity.ID" />
    <wt:row align="AlignEnum.Right">
        <wt:submitbutton theme=" ButtonThemeEnum.Warm" text="删除"/>
        <wt:closebutton />
    </wt:row>
</wt:form>
