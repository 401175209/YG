@model YG.AdminArea.ViewModels.AdminVMs.AdminListVM


<yg:treecontainer items="AllOrgan" id-field="Searcher.OrganId" height="500">

    <wt:searchpanel vm="@Model" reset-btn="true">
        <wt:row items-per-row="ItemsPerRowEnum.Three">
           
            <wt:textbox field="Searcher.ITCode" />
            <wt:textbox field="Searcher.Name" />
            <wt:combobox field="Searcher.IsValid" empty-text="全部" />
            <wt:combobox field="Searcher.SelectedUserRolesIDs" items="Searcher.AllUserRoless" empty-text="全部" />
            <wt:combobox field="Searcher.SelectedUserGroupsIDs" items="Searcher.AllUserGroupss" empty-text="全部" />
            <wt:hidden field="Searcher.OrganId" />
        </wt:row>
    </wt:searchpanel>
    <wt:grid vm="@Model" url="/AdminArea/Admin/Search" />
</yg:treecontainer>
